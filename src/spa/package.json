{
  "name": "triptime",
  "version": "0.0.0",
  "description": "TripTime is an application for groups of friends to plan a trip together. Users will be able to collaborate in real-time plotting activities over a geographical map interface.",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx}\" --max-warnings=0 --no-error-on-unmatched-pattern",
    "lint:fix": "eslint \"**/*.{js,jsx}\" --max-warnings=0 --no-error-on-unmatched-pattern --fix",
    "prettier": "prettier --write \"**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafiazman/TripTime.git"
  },
  "author": "Rafi Azman, Anran Niu, Shakeel Khan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rafiazman/TripTime/issues"
  },
  "homepage": "https://github.com/rafiazman/TripTime",
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.11.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "leaflet": "^1.6.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.5",
    "next": "^9.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-calendar": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.6.3",
    "react-leaflet-search": "^1.0.3",
    "react-timeago": "^4.4.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "cypress": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "1.19.1",
    "test-listen": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run lint"
    }
  }
}
